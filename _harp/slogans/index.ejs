<div class="button-grid">

  <% var n = 0;
    for(var key in public.slogans._data){
    n++ %>
    <div class="snippet">
      <button class="btn" onclick="play()"><%= public.slogans._data[key].word %></button>
      <audio id="audio" src="/audio/confianca.wav" ></audio>
      <!-- <a href="/slogans/<%= key %>.html" class="red"><%= public.slogans._data[key].link %></a> -->
    </div>
  <% } %>

</div>

<script type="text/javascript">
  function play() {
    var audio = document.getElementById("audio");
    audio.play();
  }
</script>
